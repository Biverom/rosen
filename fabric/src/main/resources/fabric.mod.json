{
    "schemaVersion": 1,
    "id": "rosen",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "Firch",
        "Biverom"
    ],
    "contact": {
        "homepage": "${homepage_url}",
        "issues": "${git_url}/issues",
        "sources": "${git_url}"
    },
    "license": "${license}",
    "icon": "${mod_id}_icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.notjang.rosen.Rosen"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=17"
    },
    "suggests": {
        "another-mod": "*"
    },
    "accessWidener" : "${mod_id}.accesswidener"
}
  